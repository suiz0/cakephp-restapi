{
    "name": "kinbalam/cakephp-restapi",
    "description": "RestAPI plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.7.0",
        "cakephp/authentication": "1.1.x",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kinbalam\\RestAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kinbalam\\RestAPI\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    }
}
